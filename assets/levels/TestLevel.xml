<?xml version="1.0" encoding="UTF-8"?>
<Level>
	<!--
	El tiempo se pone en segundos
	spawnMin y spawnMax es la frecuencia de spawn de flechas, sera un numero entre spawnMin y spawnMax segundos
	-->
	<caract tiempo="5" puntos="100" vidas="1" health="200" spawnMin="1" spawnMax="2.3" />
	
	<!--
	La prob de una flecha especial no deberÃ­a ser mayor de 20
	La suma de prob de todas las flechas debe ser 100
	Las flechas que no aparezcan en un nivel no estarÃ¡n en esta lista (ni con prob="0")
	-->
	<arrow type="normal" prob="50" vel="1"/>
	<arrow type="fire" prob="10" vel="1.5"/>
	<arrow type="ice" prob="10" vel="1.5"/>
	<arrow type="plant" prob="10" vel="1.5"/>
	<arrow type="fast" prob="10" vel="3"/>
	<arrow type="elec" prob="10" vel="1.5"/>
	
	<panel type="fog">
		<status>true</status>
	</panel>
	
	<!--
	En el caso de que el nivel sea por combos, estos seran los tipos necesarios y la cantidad de cada uno
		(Comprobar la clase objects.Arrow -> Strings de los TYPE_C
	Si el nivel no es de tipo combo, no hace falta poner ninguno
	-->
	<victory type="c_fire" num="2"/>
	<victory type="c_elec" num="2"/>
	<victory type="c_fast" num="2"/>
	<victory type="c_fireice" num="2"/>
	
	<!--
	platf es el ancho total de la plataforma
	-->
	<soldier x="0" y="500" arm="3" platf="300" />
</Level>